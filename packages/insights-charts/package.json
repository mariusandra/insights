{
  "name": "insights-charts",
  "description": "Desktop and Self-Hosted \"SQL-not-required\" data analytics and visualisation tool.",
  "version": "0.0.22",
  "homepage": "",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "esnext": "src/index.js",
  "keywords": [
    "insights"
  ],
  "author": {
    "name": "Marius Andra",
    "email": "marius.andra@gmail.com"
  },
  "contributors": [],
  "repository": "mariusandra/insights",
  "bugs": {},
  "directories": {
    "lib": "app"
  },
  "engines": {
    "node": ">= 7.6.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "build": "concurrently \"npm run build:cjs\" \"npm run build:es\"",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "start": "concurrently --kill-others \"npm run start:cjs\" \"npm run start:es\"",
    "start:cjs": "cross-env BABEL_ENV=cjs babel -w src --out-dir lib",
    "start:es": "cross-env BABEL_ENV=es babel -w src --out-dir es",
    "size": "size-limit",
    "test": "jest",
    "lint": "eslint src/**",
    "prepublish": "npm run build"
  },
  "files": [
    "es",
    "lib",
    "src"
  ],
  "peerDependencies": {
    "moment": "*",
    "prop-types": "*",
    "react": "*",
    "recharts": "*"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "concurrently": "^5.0.2",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-standard": "^4.0.1",
    "moment": "2.29.4",
    "prop-types": "^15.5.10",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "recharts": "1.7.1"
  }
}
